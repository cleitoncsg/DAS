package Refatoracao3;


public class CPF {

	//final int[] cpf;
	//NovoCPF novoCPF;
	private int _1oDigito;
	private int _2oDigito;
	private int _3oDigito;
	private int _4oDigito;
	private int _5oDigito;
	private int _6oDigito;
	private int _7oDigito;
	private int _8oDigito;
	private int _9oDigito;
	private int _10oDigito;
	//private int[] numCPF;
	
	private int _11oDigito; 
	
	public CPF(int[] cpf) {
		//this.cpf = cpf;
		//novoCPF = new NovoCPF(); 
		//novoCPF.numCPF = cpf;
		set1oDigito(cpf[0]);
		set2oDigito(cpf[1]);
		set3oDigito(cpf[2]);
		set4oDigito(cpf[3]);
		set5oDigito(cpf[4]);
		set6oDigito(cpf[5]);
		set7oDigito(cpf[6]);
		set8oDigito(cpf[7]);
		set9oDigito(cpf[8]);
		set10oDigito(cpf[9]);
		set11oDigito(cpf[10]);
	}
	
	public int[] getCPF() {
		//return novoCPF.numCPF;
		return new int[]{get1oDigito(), 
				         get2oDigito(), 
				         get3oDigito(), 
				         get4oDigito(), 
				         get5oDigito(), 
				         get6oDigito(), 
				         get7oDigito(), 
				         get8oDigito(), 
				         get9oDigito(), 
				         get10oDigito(),
				         get11oDigito()};
	}
	
	public int get1oDigito() {
		return _1oDigito;
	}
	
	public void set1oDigito(int digito) {
		_1oDigito = digito;
	}
	
	public int get2oDigito() {
		return _2oDigito;
	}
	
	public void set2oDigito(int digito) {
		_2oDigito = digito;
	}

	public int get3oDigito() {
		return _3oDigito;
	}
	
	public void set3oDigito(int digito) {
		_3oDigito = digito;
	}
	
	public int get4oDigito() {
		return _4oDigito;
	}
	
	public void set4oDigito(int digito) {
		_4oDigito = digito;
	}
	

	public int get5oDigito() {
		return _5oDigito;
	}
	
	public void set5oDigito(int digito) {
		_5oDigito = digito;
	}

	public int get6oDigito() {
		return _6oDigito;
	}
	
	public void set6oDigito(int digito) {
		_6oDigito = digito;
	}

	public int get7oDigito() {
		return _7oDigito;
	}
	
	public void set7oDigito(int digito) {
		_7oDigito = digito;
	}

	public int get8oDigito() {
		return _8oDigito;
	}
	
	public void set8oDigito(int digito) {
		_8oDigito = digito;
	}

	public int get9oDigito() {
		return _9oDigito;
	}
	
	public void set9oDigito(int digito) {
		_9oDigito = digito;
	}

	public int get10oDigito() {
		return _10oDigito;
	}
	
	public void set10oDigito(int digito) {
		_10oDigito = digito;
	}

	public int get11oDigito() {
		return _11oDigito;
	}
	
	public void set11oDigito(int digito) {
		_11oDigito = digito;
	}
}
